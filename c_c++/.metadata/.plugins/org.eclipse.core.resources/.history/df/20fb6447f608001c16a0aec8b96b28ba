#include <stdio.h>

int main() {
	int cha;
	while ((cha=getchar()) != EOF){
		if (cha==' ')
			printf("%d", ' ');
	}

	int error_code;
	error_code = (getchar() != EOF);
	printf("Hello World! %d\n%d",error_code,EOF); /* prints !!!Hello World!!! */
	return 0;
}
